        <Button x:Name="Powrot" Content="Powrot" Grid.Column="0" HorizontalAlignment="Stretch" Margin="0,0,0,0" Grid.Row="0" VerticalAlignment="Stretch" Width="50" FontFamily="Lucida Sans Unicode" FontSize="10" Background="#FFAFEC2B" IsHitTestVisible="True" Click="Powrot_Click"/>
    </Grid>
</Window>
